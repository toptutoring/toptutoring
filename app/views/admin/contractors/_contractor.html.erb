<tr id="row_user_<%= contractor.id %>" class="<%= "archived-row" if contractor.archived? %>">
  <td><%= contractor.full_name %></td>
  <td class="not-visible-xs text-center"><%= l contractor.created_at, format: :date %></td>
  <td class="not-visible-xs"><%= phone_number_display(contractor) %></td>
  <td class="not-visible-xs"><%= contractor.email %></td>
  <td class="not-visible-xs">$<%= contractor.contractor_account.hourly_rate %></td>
  <td class="text-center">
    <% admin_user_actions(contractor, "contractor") %>
  </td>
</tr>
