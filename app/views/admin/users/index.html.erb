<%= render "layouts/page_widget", title: "Users" do %>
  <div class="table-responsive">
    <table id="admin-user-table" class="table table-hover dt-responsive nowrap">
      <thead>
        <tr>
          <th class="col-xs-4">Name</th>
          <th>Email</th>
          <th>Action</th>
        </tr>
      </thead>
      <tbody>
        <%= render partial: "user", collection: @users %>
      </tbody>
    </table>
  </div>
<% end %>
