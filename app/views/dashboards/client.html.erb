<%= render "menu" %>
<% unless current_user.enabled? %>
  <%= render "users/set_up_account" %>
<% end %>
<% if current_user.enabled? %>
  <% if !current_user.is_student? %>
   <% if current_user.students&.first&.student_engagements&.first&.pending? || current_user.client_engagements&.first&.pending? %>
      <%= render "student_pending_engagement_message" %>
    <% end %>
  <% end %>
  <% if current_user.is_student? && current_user.client_engagements&.first&.pending? %>
    <%= render "pending_engagement_message" %>
  <% end %>
<% end %>
